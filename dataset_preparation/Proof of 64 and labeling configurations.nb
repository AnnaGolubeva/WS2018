Cell["\<\
We\[CloseCurlyQuote]ll define the following operation, which receives n group \
generators and calculates n^2 group elements by multipling them all. Then \
returns the flattened array with no repetition. Include the identity please.\
\>", "Text",
 CellChangeTimes->{{3.725589545024583*^9, 3.725589600792804*^9}, {
  3.7255899767689238`*^9, 
  3.725589980817527*^9}},ExpressionUUID->"cb1bae5d-6242-4af6-87c1-\
607202a2eff8"],

Cell[BoxData[
 RowBox[{
  RowBox[{"groupSquared", " ", "[", "listOfGenerators_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"listSquared", ",", "length"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"length", " ", "=", 
      RowBox[{"Length", "[", "listOfGenerators", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"listSquared", " ", "=", " ", 
      RowBox[{
       RowBox[{"Flatten", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
            "listOfGenerators", "\[LeftDoubleBracket]", "i", 
             "\[RightDoubleBracket]"}], ".", 
            RowBox[{
            "listOfGenerators", "\[LeftDoubleBracket]", "j", 
             "\[RightDoubleBracket]"}]}], " ", ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "1", ",", "length"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"j", ",", "1", ",", "length"}], "}"}]}], "]"}], ",", 
         "1"}], "]"}], "//", "Union"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "listSquared", "]"}], ";"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.7255895364117293`*^9, 3.725589541575571*^9}, {
  3.7255896043995996`*^9, 3.7255898101376247`*^9}, {3.7255898462909765`*^9, 
