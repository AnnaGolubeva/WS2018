        "points"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"DeleteCases", "[", 
       RowBox[{
        RowBox[{"Flatten", "[", 
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
             "a", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], "*", 
             RowBox[{
             "a", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}], "*", 
             RowBox[{"Distances", "\[LeftDoubleBracket]", 
              RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "1", ",", 
              RowBox[{"Length", "[", "a", "]"}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"j", ",", "1", ",", 
              RowBox[{"Length", "[", "a", "]"}]}], "}"}]}], "]"}], ",", "1"}],
          "]"}], ",", "0"}], "]"}], "//", "Sort"}]}], "]"}]}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"DistanceTuple", "[", "x", "]"}], " ", "=", " ", 
