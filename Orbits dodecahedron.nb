Cell["\<\
We will now check the code Lauren did, with computing independent, sorted, \
\[OpenCurlyDoubleQuote]distance tuples\[CloseCurlyDoubleQuote]. We cheat a \
bit in the code so that the code outputs {-1} for the all-zero configuration.\
\>", "Text",
 CellChangeTimes->{{3.7256217944017763`*^9, 3.7256217996172123`*^9}, {
  3.7256221458028555`*^9, 3.72562217471463*^9}, {3.7256230707971497`*^9, 
  3.7256230977979765`*^9}},ExpressionUUID->"709c493a-c570-4170-921a-\
a56267f75ff8"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"DistanceTuple", "[", "a_", "]"}], ":=", 
   RowBox[{"If", "[", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{"if", " ", "all", " ", "spins", " ", "are", " ", "zero"}], ",", 
      " ", 
      RowBox[{"output", " ", 
       RowBox[{"{", 
        RowBox[{"-", "1"}], "}"}]}]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"Plus", "@@", "a"}], "\[Equal]", "0"}], ",", 
     RowBox[{"{", 
      RowBox[{"-", "1"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Else", ",", " ", 
       RowBox[{
       "output", " ", "the", " ", "distance", " ", "between", " ", 
        "points"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"DeleteCases", "[", 
       RowBox[{
        RowBox[{"Flatten", "[", 
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
             "a", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], "*", 
             RowBox[{
             "a", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}], "*", 
             RowBox[{"Distances", "\[LeftDoubleBracket]", 
              RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "1", ",", 
              RowBox[{"Length", "[", "a", "]"}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"j", ",", "1", ",", 
              RowBox[{"Length", "[", "a", "]"}]}], "}"}]}], "]"}], ",", "1"}],
          "]"}], ",", "0"}], "]"}], "//", "Sort"}]}], "]"}]}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"DistanceTuple", "[", "x", "]"}], " ", "=", " ", 
     RowBox[{
      RowBox[{"{", "1", "}"}], " ", "if", " ", "x", " ", "refers", " ", "to", 
      " ", "the", " ", "configuration", " ", "of", " ", "2", " ", "spins", 
      " ", "up", " ", "in", " ", "neighboring", " ", 
      RowBox[{"faces", ".", " ", "That"}], " ", "is"}]}], ",", " ", 
    RowBox[{
     RowBox[{
     "they", " ", "have", " ", "distance", " ", "1", " ", "between", " ", 
      "them"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{
     "Let", " ", "y", " ", "be", " ", "a", " ", "configuration", " ", "with", 
      " ", "3", " ", "spins"}]}], ",", " ", 
    RowBox[{
     RowBox[{
     "all", " ", "with", " ", "distance", " ", "1", " ", "between", " ", 
      RowBox[{"them", ".", " ", "Then"}], " ", "we", " ", "also", " ", 
      RowBox[{"have", ":", "\[IndentingNewLine]", 
       RowBox[{"DistanceTuple", "[", "y", "]"}]}]}], " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"1", ",", "1", ",", "1"}], "}"}]}]}], "\[IndentingNewLine]", 
   "*)"}], "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{{3.725622204894927*^9, 3.7256222108718224`*^9}, {
  3.7256222450287013`*^9, 3.7256223987909117`*^9}, {3.7256224633223357`*^9, 
  3.725622465705679*^9}, {3.725622513608077*^9, 3.7256225153416924`*^9}, {
  3.7256227178205423`*^9, 3.725622719395728*^9}, {3.7256229384096613`*^9, 
  3.7256229609320145`*^9}, {3.725623101197049*^9, 3.7256232522584934`*^9}, {
  3.725624196022084*^9, 
  3.7256242365866575`*^9}},ExpressionUUID->"1056fe9d-1658-4809-ab9b-\
38782391e39d"],

